{
  "enabledManagers": ["dockerfile", "tekton"],
  "dockerfile": {
    "enabled": true,
    "ignoreTests": true,
    "automergeType": "pr",
    "automerge": true,
    "platformAutomerge": true
  },
  "tekton": {
    "enabled": true,
    "fileMatch": [".yaml$", ".yml$", ".env$"],
    "automergeType": "pr",
    "ignoreTests": true,
    "automerge": true,
    "platformAutomerge": true
  }
}
