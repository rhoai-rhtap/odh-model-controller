{
  "enabledManagers": ["containerfile", "tekton"],
  "containerfile": {
    "enabled": true,
    "ignoreTests": true,
    "automergeType": "pr",
    "automerge": true
  },
  "tekton": {
    "enabled": true,
    "fileMatch": ["\\.yaml$", "\\.yml$"],
    "automergeType": "pr",
    "ignoreTests": true,
    "automerge": true
  }
}
